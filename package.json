{
  "name": "metaterran",
  "description": "A GatsbyJS blog, hosted on Linode, shipped in a Vagrant, built with CircleCI, and tested with Jest BDD.",
  "version": "1.0.0",
  "author": "Sajjad Hossain <sajjad@withpulp.com>",
  "bugs": {
    "url": "https://github.com/sajjadhossain/metaterran/issues"
  },
  "dependencies": {
    "babel-preset-react": "^6.24.1",
    "debug": "^4.0.1",
    "gatsby": "^2.0.7",
    "gatsby-plugin-catch-links": "^2.0.2",
    "gatsby-plugin-feed": "^2.0.5",
    "gatsby-plugin-google-analytics": "^2.0.5",
    "gatsby-plugin-manifest": "^2.0.2",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-postcss": "^2.0.0",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sass": "^2.0.1",
    "gatsby-plugin-sharp": "^2.0.5",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-images": "^2.0.1",
    "gatsby-remark-prismjs": "^3.0.0",
    "gatsby-remark-responsive-iframe": "^2.0.5",
    "gatsby-remark-smartypants": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.1",
    "gatsby-transformer-remark": "^2.1.1",
    "gatsby-transformer-sharp": "^2.1.1",
    "lodash": "^4.17.11",
    "postcss-import": "^12.0.0",
    "postcss-preset-env": "^6.0.3",
    "prismjs": "^1.15.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.13",
    "typeface-merriweather": "0.0.43",
    "typeface-montserrat": "0.0.43",
    "typography": "^0.16.17",
    "typography-theme-wordpress-2016": "^0.15.10"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^1.2.0",
    "jest": "^23.5.0",
    "jest-puppeteer": "^3.3.1",
    "node-sass": "^4.9.3",
    "prettier": "^1.13.7",
    "puppeteer": "^1.7.0",
    "sass-loader": "^7.1.0",
    "util.promisify": "^1.0.0"
  },
  "homepage": "https://github.com/sajjadhossain/metaterran#readme",
  "keywords": [
    "gatsby",
    "circleci",
    "react",
    "jest"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sajjadhossain/metaterran.git"
  },
  "scripts": {
    "dev": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "rm -rf ./.cache && rm -rf ./public && rm -rf ./node_modules",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "./node_modules/.bin/jest jest/*.test.js",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js"
  }
}
